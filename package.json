    "jest": "^25.1.0",
    "lint-staged": "^9.4.3",
    "mocha": "^6.2.2",
    "parcel": "^1.12.4",
    "prettier": "^1.19.1",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "bump-patch": "yarn version patch",
    "build": "parcel build ./lib/index.js",
    "docs-build": "documentation build lib/index.js -f md -o docs.md",
    "docs-buildh": "documentation build lib/index.js -f html -o docs --watch",
