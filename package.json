    "babel-strip-test-code": "^1.1.12",
    "chai": "^4.2.0",
    "documentation": "^12.1.4",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-strongloop": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.1",
